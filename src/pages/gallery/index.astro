---
import Layout from '../../layouts/Layout.astro';
const lunettes = []
---


<Layout title="Galerie">
  <main class="w-full min-h-screen px-2 md:px-12 py-8 md:py-16 bg-base-100 font-[Arial,Helvetica,sans-serif]">
    <h1 class="text-center text-base-content font-medium text-lg mb-14 mt-4">Vos créations sauvegardées</h1>
    <div>
    { lunettes.map((lunette) => <Card {...lunette}/>)}
    </div>
  </main>
</Layout>
